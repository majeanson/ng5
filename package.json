		"svelte": "^4.0.5",
		"svelte-check": "^3.4.5",
		"tslib": "^2.6.0",
		"typesafe-i18n": "^5.24.3",
		"typescript": "^5.1.6",
		"vite": "^4.4.2"
	},
	"type": "module"
}
